<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-requires about_brazo_dialog 1.0 -->
  <!-- interface-naming-policy project-wide -->
  <object class="AboutBrazoDialog" id="about_brazo_dialog">
    <property name="border_width">5</property>
    <property name="window_position">center</property>
    <property name="icon">../media/icon.svg</property>
    <property name="type_hint">dialog</property>
    <property name="has_separator">True</property>
    <property name="program_name">Brazo</property>
    <property name="copyright">Copyright 2011 Nick Pascucci</property>
    <property name="comments" translatable="yes">Control and configuration for robot arms.</property>
    <property name="license">Copyright 2011, Nicholas Pascucci.
All Rights Reserved.</property>
    <property name="authors">Nick Pascucci</property>
    <property name="logo">../media/icon.svg</property>
    <property name="wrap_license">True</property>
    <child internal-child="vbox">
      <object class="GtkVBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="spacing">2</property>
        <child internal-child="action_area">
          <object class="GtkHButtonBox" id="dialog-action_area1">
            <property name="visible">True</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
